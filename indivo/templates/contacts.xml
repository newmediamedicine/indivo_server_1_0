{% spaceless %}
<Contact id="{{ contact.id }}" xmlns="http://indivo.org/vocab/xml/documents#">
    <name>
        <fullName>{{ contact.full_name }}</fullName>
        <givenName>{{ contact.given_name }}</givenName>
        <familyName>{{ contact.family_name }}</familyName>
    </name>
</Contact>
{% endspaceless %}
