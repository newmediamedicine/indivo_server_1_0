+								</xs:documentation>
+							</xs:annotation>
+						</xs:element>
+						<xs:element name="site" type="indivo:CodedValue" minOccurs="0" maxOccurs="1">
+							<xs:annotation>
+								<xs:documentation xml:lang="en">Site where the device was applied. For example, right arm. Optional
+								</xs:documentation>
+							</xs:annotation>
+						</xs:element>
+					</xs:sequence>
+				</xs:complexType>
+			</xs:element>
+		</xs:sequence>
+	</xs:complexType>
+
